{
  "name": "@chrisaguilar/pword",
  "version": "0.0.3",
  "description": "A password generator + manager",
  "main": "index.js",
  "bin": {
    "pword": "index.js"
  },
  "scripts": {
    "lint": "tslint --project tsconfig.json",
    "postinstall": "node ./init.js",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chrisaguilar/pword.git"
  },
  "keywords": [
    "password",
    "manager",
    "generator",
    "typescript",
    "TS",
    "jest"
  ],
  "author": "Christopher Aguilar <christopher.m.aguilar@gmail.com> (https://github.com/chrisaguilar)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chrisaguilar/pword/issues",
    "email": "christopher.m.aguilar@gmail.com"
  },
  "homepage": "https://github.com/chrisaguilar/pword",
  "engines": {
    "node": ">=7.4.0"
  },
  "preferGlobal": true,
  "dependencies": {
    "fs-promise": "^2.0.0",
    "minimist": "^1.2.0",
    "ts-node": "^2.1.0",
    "tsconfig-paths": "^2.1.1",
    "typescript": "^2.1.6"
  },
  "devDependencies": {
    "@types/fs-promise": "^1.0.1",
    "@types/jest": "^18.1.1",
    "@types/minimist": "^1.2.0",
    "@types/node": "^7.0.5",
    "jest": "^19.0.0",
    "ts-jest": "^19.0.0",
    "tslint": "^4.4.2",
    "tslint-eslint-rules": "^3.3.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "notify": true,
    "moduleDirectories": ["node_modules", "./src/"],
    "transform": {
      ".(ts|tsx)": "ts-jest/preprocessor"
    },
    "testMatch": [ "<rootDir>/__tests__/**/*.ts?(x)" ],
    "verbose": false
  }
}
